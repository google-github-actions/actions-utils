{
  "name": "actions-utils",
  "version": "0.1.0",
  "description": "Helpers and utilities that are shared among Google GitHub Actions",
  "private": true,
  "author": "Google LLC",
  "license": "Apache-2.0",
  "scripts": {
    "build": "rm -rf dist/ && ncc build --minify --source-map --no-source-map-register src/index.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write **/*.ts",
    "docs": "rm -rf docs/ && typedoc",
    "test": "mocha -r ts-node/register -t 120s 'tests/**/*.test.ts'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/google-github-actions/actions-utils"
  },
  "files": [
    "CONTRIBUTING.md",
    "LICENSE.md",
    "README.md",
    "dist/"
  ],
  "module": "./dist/index.js",
  "main": "./dist/index.js",
  "types": "./dist/index.d.js",
  "dependencies": {
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@types/chai": "^4.3.x",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "@vercel/ncc": "0.33.1",
    "chai": "^4.3.x",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint": "^8.4.1",
    "mocha": "^9.1.3",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "typedoc-plugin-markdown": "^3.11.7",
    "typedoc": "^0.22.10",
    "typescript": "^4.5.4"
  }
}
