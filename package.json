{
  "name": "@google-github-actions/actions-utils",
  "version": "0.4.2",
  "description": "Helpers and utilities that are shared among Google GitHub Actions",
  "author": "Google LLC",
  "license": "Apache-2.0",
  "scripts": {
    "build": "rm -rf dist/ && ncc build --minify --source-map --no-source-map-register src/index.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write **/*.ts",
    "docs": "rm -rf docs/ && typedoc",
    "test": "mocha -r ts-node/register -t 120s 'tests/**/*.test.ts'",
    "all": "npm run format && npm run lint && npm run docs && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/google-github-actions/actions-utils"
  },
  "files": [
    "CONTRIBUTING.md",
    "LICENSE.md",
    "README.md",
    "dist/"
  ],
  "module": "./dist/index.js",
  "main": "./dist/index.js",
  "types": "./dist/index.d.js",
  "dependencies": {
    "yaml": "^2.1.1"
  },
  "devDependencies": {
    "@types/chai": "^4.3.x",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "@vercel/ncc": "0.34.0",
    "chai": "^4.3.x",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "mocha": "^10.0.0",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.8",
    "typedoc-plugin-markdown": "^3.13.3",
    "typescript": "^4.7.4"
  }
}
